# API Server Configuration
EXPO_PUBLIC_API_URL=https://your-api-server.com
EXPO_PUBLIC_SOCKET_URL=wss://your-api-server.com

# Google Maps API
EXPO_PUBLIC_GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# App Configuration
EXPO_PUBLIC_APP_NAME=SmartBus
EXPO_PUBLIC_APP_VERSION=1.0.0

# Environment
NODE_ENV=production

# Push Notifications (optional)
EXPO_PUBLIC_PUSH_PROJECT_ID=your_expo_push_project_id

# Analytics (optional)
EXPO_PUBLIC_GA_ID=your_google_analytics_id
EXPO_PUBLIC_SENTRY_DSN=your_sentry_dsn

# Feature Flags
EXPO_PUBLIC_ENABLE_ANALYTICS=true
EXPO_PUBLIC_ENABLE_NOTIFICATIONS=true
EXPO_PUBLIC_ENABLE_LOCATION_TRACKING=true

# App Store Configuration
EXPO_PUBLIC_APP_STORE_ID=your_app_store_id
EXPO_PUBLIC_PLAY_STORE_ID=your_play_store_id 